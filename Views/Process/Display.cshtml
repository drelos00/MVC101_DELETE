@model System.Diagnostics.Process

@* This is a c# island *@
@{
    ViewData["Title"] = "Process Details";
}
<div class="text-center">
    <table>
        <tr>
            <td>ID</td>
            <td>@Model.Id</td>
        </tr>
        
        <tr>
            <td>Name</td>
            <td>@Model.ProcessName</td>
        </tr>
        
        <tr>
            <td>Count</td>
            <td>@Model.HandleCount</td>
        </tr>
        
    </table>
    
    

</div>
